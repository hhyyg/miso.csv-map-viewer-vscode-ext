{
	"name": "csv-map-viewer",
	"displayName": "CSV Map Viewer",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:csv-map-viewer.show"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "csv-map-viewer.show",
				"title": "Show CSV Map information"
			}
		],
		"configuration": {
			"title": "CSV Map Viewer",
			"properties": {
				"csv-map-viewer.map": {
					"type": "object",
					"default": {
						"YOUR FIRST COLUMN TEXT": {
							"0": { "title" : "title-YOUR FIRST COLUMN TEXT-0", "description": "description-YOUR FIRST COLUMN TEXT-A-0" },
							"1": { "title" : "title-YOUR FIRST COLUMN TEXT-1", "description": "description-YOUR FIRST COLUMN TEXT-A-1" }
						},
						"A": {
							"0": { "title" : "title-A-0", "description": "description-A-0" },
							"1": { "title" : "title-A-1", "description": "description-A-1" }
						},
						"B": {
							"0": { "title" : "title-B-0", "description": "description-B-0" },
							"1": { "title" : "title-B-1", "description": "description-B-1" }
						},
						"0": {
							"0" : { "title" : "title-0-0", "description": "description-0-0" },
							"1" : { "title" : "title-0-1", "description": "description-0-1" },
							"2" : { "title" : "title-0-2", "description": "description-0-2" }
						},
						"1": {
							"0" : { "title" : "title-1-0", "description": "description-1-0" }
						},
						"2": {
							"0" : { "title" : "title-2-0", "description": "description-2-0" }
						}
					},
					"description": "Your CSV Map information"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}